#include<stdio.h>
int main()
{
	int a[100],i,n,num;
    printf("enter limit");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
	   printf("enter n numbers");
       scanf("%d",&a[i]);
    }
     printf("enter num to search");
     scanf("%d",&num);
     for(i=0;i<n;i++)
     {
	   if(a[i]==num)
       {
	     printf("found");
         break;
        }
     }
       if(i==n)
       printf("not found");
}
